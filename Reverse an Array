//You are given an array of integers arr[]. Your task is to reverse the given array.
class Solution {
public:
    void reverseArray(vector<int>& arr) {
        int start = 0;
        int end = arr.size() - 1;

        while (start < end) {
            swap(arr[start], arr[end]); // Use the correct variable name 'arr' and close with a semicolon
            start++;
            end--;
        }
    }
};
